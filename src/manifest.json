{
    "manifest_version": 3,
    "name": "Sklep Kolonialny w święta",
    "version": "1.0",
    "description": "Usuwa komunikat o niedostępności sklepu Wojciecha Cejrowskiego w weekendy i święta, pozwala na swobodne przeglądanie strony.",
    "content_scripts": [
      {
        "matches": ["https://cejrowski.com/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "128": "images/icon128.png"
    }
  }
  